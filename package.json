{
  "name": "ng-localizacao",
  "version": "0.0.1",
  "engines": {
    "node": "14"
  },
  "peerDependencies": {
    "@angular/common": "~11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/http": "^7.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apsoftwaresi/ng-localizacao.git"
  },
  "author": {
    "name": "Caique Pimenta",
    "email": "cpimenta.apsoftware@gmail.com"
  },
  "homepage": "https://apsoftwaresi.github.io/ng-localizacao/",
  "keywords": [
    "angular",
    "viacep",
    "buscar cep",
    "cep brasil",
    "endereços brasil",
    "api cep",
    "geolocalização",
    "latitude",
    "longitude",
    "coordenada",
    "openstreetmap",
    "osm",
    "mapbox",
    "logradouro",
    "bairro",
    "cidade",
    "uf",
    "estado",
    "ddd",
    "município",
    "código ibge",
    "complemento",
    "gia",
    "siafi"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apsoftwaresi/ng-localizacao.git/issues"
  },
  "scripts": {
    "ng": "ng",
    "config": "ts-node --project projects/ng-localizacao-amostra/tsconfig.app.json projects/ng-localizacao-amostra/set-env.ts",
    "start-dev": "ng serve -o",
    "prestart": "cp README.md projects/ng-localizacao-amostra/src/assets/README.md",
    "start": "npm run config && ng serve",
    "build": "ng build",
    "build:lib": "npm run build ng-localizacao",
    "build:lib:prod": "npm run build:lib -- --prod",
    "test": "npm run test:lib",
    "test:lib": "ng test ng-localizacao",
    "lint": "npm run lint:lib && npm run lint:amostra",
    "lint:lib": "ng lint ng-localizacao --fix",
    "lint:amostra": "ng lint ng-localizacao-amostra --fix",
    "e2e": "ng e2e",
    "exec": "npm run build:lib:prod && ng serve",
    "page-build": "npm run config && ng build --prod --base-href \"https://apsoftwaresi.github.io/ng-localizacao/\"",
    "page-deploy": "ngh --no-silent --dir dist/ng-localizacao-amostra",
    "github-page": "npm run build:lib:prod && npm run page-build && npm run page-deploy",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/http": "^7.0.4",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.11",
    "dotenv": "^8.2.0",
    "envalid": "^7.1.0",
    "jquery": "^3.5.0",
    "ngx-markdown": "^11.0.1",
    "popper.js": "^1.14.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "^11.2.6",
    "@angular/compiler-cli": "~11.2.5",
    "@angular/language-service": "^11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}